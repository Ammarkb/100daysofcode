#include <iostream>
using namespace std;

int main()
{
	int n,d;

	cout<<"Enter the value of n and d: "<<endl;
	cin>>n>>d;
	int a[n];

	cout<<"Enter the array elements : ";
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}


	cout<<"Array elements after rotation : ";
	for(int i=0;i<n;i++)
	{
		cout<<a[(i+d)%n]<<" ";
	}

	return 0;
}


            OUTPUT
Enter the value of n and d: 6  2
Enter the array elements: 4 7 1 0 3 8
Array elements after rotation: 1 0 3 8 4 7
